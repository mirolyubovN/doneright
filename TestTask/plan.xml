<root>
	<testPlan>
		<testScript>
			<testCase>
				<actionSteps>
					<open>http://www.ng.kz/</open>
				</actionSteps>
				<actionSteps>
					<getNode>//*[@id=&quot;main-layout&quot;]/div[3]/div[5]/div[3]/div[2]/div[2]/form/div[2]/label/a, myButton</getNode>
				</actionSteps>
				<actionSteps>
					<click>myButton</click>
				</actionSteps>
				<actionSteps>
					<getNode>//*[@id=&quot;upload-button&quot;], myButtonTwo</getNode>
				</actionSteps>
				<actionSteps>
					<click>myButtonTwo</click>
				</actionSteps>
				<actionSteps>
					<exec>alert(document.getElementById('date-time').getAttribute('value'));</exec>
				</actionSteps>
				<assertEquals>
					<opened>yes</opened>
				</assertEquals>
				<assertEquals>
					<myButton>yes</myButton>
				</assertEquals>
				<assertEquals>
					<click>yes</click>
				</assertEquals>
				<assertEquals>
					<myButtonTwo>yes</myButtonTwo>
				</assertEquals>
				<assertEquals>
					<click>yes</click>
				</assertEquals>
				<assertEquals>
					<exec>yes</exec>
				</assertEquals>
			</testCase>
